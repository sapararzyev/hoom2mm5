<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.hoom2mm5.ui.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_sekondFragment2"
            app:destination="@id/sekondFragment2" />
    </fragment>

    <fragment
        android:id="@+id/sekondFragment2"
        android:name="com.example.hoom2mm5.ui.SekondFragment"
        android:label="fragment_sekond"
        tools:layout="@layout/fragment_sekond">
        <argument
            android:name="fname"
            app:argType="string" />
        <argument
            android:name="sname"
            app:argType="string" />
    </fragment>

</navigation>